ODM(VAL) ; Validate and transform a obsteric delivery method
 S VAL=$$UP^XLFSTR($G(VAL))
 Q $S("^V^C^O^"[(U_VAL_U):VAL,VAL["VAGIN":"V",VAL["CESAR":"C",1:"O")
 ;
OUT ; Update Pregnancy Outcome
 ;
 ; OutcomeType
 ;    Unknown                  U
 ;    FullTermDelivery         F
 ;    PretermDelivery          P
 ;    SpontaneousAbortion      AS *** Spontaneous
 ;    Stillbirth               S  *** Demise or Stillbirth
 ;    PregnancyTermination     AI *** Induced
 ;    Ectopic                  E
 ;
 ; VistA - DSIO PREGNANCY HISTORY - 3.6 (OUTCOME)
 ;
 Q:$G(X)=""!('$G(DFN))
 N VAL
 S X=$$UP^XLFSTR($G(X))
 S VAL=$S("^E^AI^AS^S^F^U^P^"[(U_X_U):X,1:"")
 S:VAL="" VAL=$S(X["UNKNOWN":"U",X["FULLTERM":"F",X["PRETERM":"P",X["SPONTANEOUS":"AS",1:"")
 S:VAL="" VAL=$S(X["STILLBIRTH":"S",X["TERMINATION"!(X["INDUCED"):"AI",X["ECTOPIC":"E",1:"")
 S X=VAL D PREG(3.6)
 Q
