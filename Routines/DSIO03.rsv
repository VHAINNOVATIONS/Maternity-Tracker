 S FWK=+$P(FLD,"W"),FDY=+$P($P(FLD,"D"),"W",2)
 S IPT(19641.13,PREG_",",3.1)=$S(TYP="W":X_"W"_FDY_"D",TYP="D":FWK_"W"_X_"D",1:"")
 D UPDATE^DIE(,"IPT")
 Q
