O(DFN,OBJECT,REL,CAT,CODE,SYS,DIS,VAL,NEG,NAR) ; Generic Entry
 ;
 ;    DFN = PATIENT
 ; OBJECT = OBJECTS OF OBSERVATION (ARRAY)
 ;          FB.IEN (FETUS/BABY)
 ;          PG.IEN (PREGNANCY)
 ;    REL = RELATIONSHIP
 ;    CAT = CATEGORY
 ;   CODE = SYS NAME^SYS VALUE^CODE^DISPLAY NAME
 ;    VAL = TYPE^UNIT^VALUE
 ;    NEG = NEGATION (T/F)
 ;    NAR = NARRATIVE (ARRAY OF TEXT)
 ;  **SYS = $S(SYS="LNC":"LOINC",SYS="SCT":"SNOMED-CT",1:SYS)
 ;
 Q:'$G(DFN)!($G(CODE)="")
 N REF,OUT,CT
 S REF(0)="TIU."_+DDCSR
 S REL=$G(REL),CAT=$G(CAT),DIS=$G(DIS),VAL=$G(VAL),NEG=$G(NEG)
 S SYS=$S($G(SYS)'="":SYS,1:"OTHER")
 D OBS^DSIO10(.OUT,,DFN,$$NOW^XLFDT,.OBJECT,.REF,,CAT,SYS_"^^"_CODE_U_DIS,"^^"_VAL,,,,.NAR,1)
 Q
