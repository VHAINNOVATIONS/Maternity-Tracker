@model VA.Gov.Artemis.UI.Data.Models.Education.PatientEducationItemDetail

@{
    ViewBag.Title = "MCC Dashboard - Patient Education Item";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

<br />

@{ 
    string header = (string.IsNullOrWhiteSpace(Model.Item.Ien)) ? "Add New Patient Education Item" : "Edit Patient Education Item";
}
<h1 class="h3 page-header">Completed Education Item Details</h1>
<br />

<div class="row">
    <div class="col-md-4">

        <table class="table table-bordered table-db table-horizontal">
            <tr>
                <th scope="row">Description:</th>
                <td>@Model.Item.Description</td>
            </tr>
            <tr>
                <th scope="row">Category:</th>
                <td>@Model.Item.Category</td>
            </tr>
            <tr>
                <th scope="row">Type:</th>
                <td>@Model.Item.ItemTypeDescription</td>
            </tr>
            @if (@Model.Item.ItemType == VA.Gov.Artemis.UI.Data.Models.Education.EducationItemType.LinkToMaterial)
            {
                <tr>
                    <th scope="row">URL:</th>
                    <td>@Model.Item.Url</td>
                </tr>
            }
            <tr>
                <th scope="row">Coding System:</th>
                <td>@Model.Item.CodingSystem</td>
            </tr>
            <tr>
                <th scope="row">Code:</th>
                <td>@Model.Item.Code</td>
            </tr>
            <tr>
                <th scope="row">Completed By:</th>
                <td>@Model.Item.CompletedBy</td>
            </tr>
            <tr>
                <th scope="row">Completed On:</th>
                <td>@Model.Item.CompletedOnDisplay</td>
            </tr>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-4">
        <div class="pull-right">
            <button type="button" class="btn btn-default" onclick="location.href='@Url.Action("PatientIndex", new { @dfn = Model.Patient.Dfn })'">Return To List</button>
        </div>
    </div>
</div>
<br />


@section Navigation
{
    @{Html.RenderPartial("~/Views/Shared/_NavGroupPatient.cshtml", Model.Patient);}
}

