@model VA.Gov.Artemis.UI.Data.Models.Consults.ConsultDetail

@{
    ViewBag.Title = "MCC Dashboard - Checklist Item Detail";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

<h1 class="h3 page-header">Consult Detail</h1>
<br />
<pre>@Model.DetailText</pre>
<br />
<div class="row">
    <div class="col-md-12">
        <div class="pull-right">
            <button type="button" class="btn btn-default" onclick="location.href='@Url.Action("Index", new {@dfn = Model.Patient.Dfn})'">Return to List</button>       
        </div>
    </div>
 </div>

<br />

@section Navigation
{
    @{Html.RenderPartial("~/Views/Shared/_NavGroupPatient.cshtml", Model.Patient);}
}



