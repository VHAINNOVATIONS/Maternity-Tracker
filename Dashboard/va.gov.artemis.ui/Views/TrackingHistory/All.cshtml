@using VA.Gov.Artemis.UI.Data.Models.Track;
@model VA.Gov.Artemis.UI.Data.Models.Track.TrackingEntryList

@{
    ViewBag.Title = "MCC Dashboard - Tracking History";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

<h1 class="h3 page-header">Tracking History</h1>

<div>
        @{
            Html.RenderPartial("_TrackingHistoryTable");
            Html.RenderPartial("~/Views/Shared/_paging.cshtml", Model.Paging); 
        }
</div>

@section Scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#nav-track-hx').addClass('active').siblings().removeClass('active');
        });
    </script>
}

@section Navigation
{
    @{Html.RenderPartial("~/Views/Shared/_NavGroupTracking.cshtml");}
}