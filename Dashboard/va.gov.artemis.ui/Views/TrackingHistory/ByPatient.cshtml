@using VA.Gov.Artemis.UI.Data.Models.Track;
@model VA.Gov.Artemis.UI.Data.Models.Track.TrackingEntryList

@{
    ViewBag.Title = "MCC Dashboard - Tracking History";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

<h1 class="h3 page-header">Tracking History - By Patient</h1>

<div>
    @{
        Html.RenderPartial("_TrackingHistoryTable");
        Html.RenderPartial("~/Views/Shared/_paging.cshtml", Model.Paging);
    }
    <br />
    <div class="row">
        <div class="col-md-12">
            <div class="pull-right">
                <button class="btn btn-default" onclick="location.href='@Model.ReturnUrl'">Return to All Patients</button>
            </div>
        </div>
    </div>
</div>

@section Scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            $('#history-btn').addClass('btn-primary');
        });
    </script>
}

@section Navigation
{
    @{Html.RenderPartial("~/Views/Shared/_NavGroupTracking.cshtml");}
}